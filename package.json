{
  "name": "desktop-jira-tracker",
  "version": "1.0.0",
  "description": "desktop jira time tracking",
  "main": "main.js",
  "scripts": {
    "hot": "cross-env HOT=1 NODE_ENV=development electron --enable-transparent-visuals --disable-gpu --ignore-certificate-errors -r babel-register -r babel-polyfill ./main.development",
    "hot-server": "node -r babel-register server.js",
    "package": "cross-env NODE_ENV=production node -r babel-register package.js",
    "package-all": "cross-env NODE_ENV=production npm run package -- --all",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "dev": "concurrently --kill-others \"npm run hot\" \"npm run hot-server\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "7zip": "0.0.6",
    "babel-core": "6.20.0",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-dev-expression": "0.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "concurrently": "3.1.0",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "del": "2.2.2",
    "electron-debug": "1.1.0",
    "electron-devtools-installer": "2.0.1",
    "electron-packager": "8.4.0",
    "electron-prebuilt": "1.4.12",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.8.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "redux-logger": "2.7.4",
    "style-loader": "0.13.1",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2",
    "webpack-merge": "1.1.1"
  },
  "dependencies": {
    "bufferutil": "^1.3.0",
    "electron": "1.4.12",
    "electron-json-storage": "2.1.0",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "jira-connector": "2.5.0",
    "jwt-simple": "0.5.1",
    "lesshat": "4.1.0",
    "moment": "2.17.1",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "5.0.1",
    "react-select": "1.0.0-rc.2",
    "redux": "3.6.0",
    "redux-form": "6.3.2",
    "redux-immutable": "3.0.9",
    "redux-thunk": "2.1.0",
    "request": "2.79.0",
    "reselect": "2.5.4",
    "socket.io-client": "^1.7.2",
    "source-map-support": "0.4.6",
    "user-media-screenshot": "1.0.2",
    "utf-8-validate": "^2.0.0"
  }
}
