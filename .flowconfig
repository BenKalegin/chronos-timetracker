[ignore]
<PROJECT_ROOT>/node_modules/*
<PROJECT_ROOT>/app/main.prod.js
<PROJECT_ROOT>/app/main.prod.js.map
<PROJECT_ROOT>/app/dist/.*
<PROJECT_ROOT>/resources/.*
<PROJECT_ROOT>/release/.*
<PROJECT_ROOT>/dll/.*
<PROJECT_ROOT>/release/.*
<PROJECT_ROOT>/git/.*

[libs]

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=enable
module.name_mapper.extension='less' -> '<PROJECT_ROOT>/app/assets/stub.js'
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
module.name_mapper='^actions$' -> '<PROJECT_ROOT>/app/actions/'
module.name_mapper='^types$' -> '<PROJECT_ROOT>/src/types/'
module.name_mapper='^components$' -> '<PROJECT_ROOT>/src/components/'
module.name_mapper='^selectors$' -> '<PROJECT_ROOT>/src/selectors/'
module.name_mapper='^styles$' -> '<PROJECT_ROOT>/src/styles/'

module.name_mapper='^api$' -> '<PROJECT_ROOT>/src/utils/api'
module.name_mapper='^config$' -> '<PROJECT_ROOT>/src/utils/config'
module.name_mapper='^data$' -> '<PROJECT_ROOT>/src/utils/data'
module.name_mapper='^external-open-util$' -> '<PROJECT_ROOT>/src/utils/external-open-util'
module.name_mapper='^jiraColors-util$' -> '<PROJECT_ROOT>/src/utils/jiraColors-util'
module.name_mapper='^log-util$' -> '<PROJECT_ROOT>/src/utils/log-util'
module.name_mapper='^normalize-util$' -> '<PROJECT_ROOT>/src/utils/normalize-util'
module.name_mapper='^screenshot-util$' -> '<PROJECT_ROOT>/src/utils/screenshot-util'
module.name_mapper='^time-util$' -> '<PROJECT_ROOT>/src/utils/time-util'
