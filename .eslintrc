{
  "parser": "babel-eslint",
    "extends": "airbnb",
    "settings": {
      "ecmascript": 6,
        "jsx": true,
      "import/resolver": "webpack",
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "env": {
      "browser": true,
        "node": true,
    },
    "rules": {
      "jsx-a11y/img-uses-alt": "off",
      "jsx-a11y/redundant-alt": "off",
      "jsx-a11y/valid-aria-role": "off",
      "new-cap": "off",
      "no-confusing-arrow": 0,
      "import/no-extraneous-dependencies": 0,
      "react/forbid-prop-types": 0,
    },
    "globals": {
      "Immutable": true,
    },
}
